<p id="notice"><%= notice %></p>

<h1>Listing Libochaos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @libochaos.each do |libochao| %>
      <tr>
        <td><%= link_to 'Show', libochao %></td>
        <td><%= link_to 'Edit', edit_libochao_path(libochao) %></td>
        <td><%= link_to 'Destroy', libochao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Libochao', new_libochao_path %>
